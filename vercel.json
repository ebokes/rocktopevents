{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://rocktopevents.onrender.com/api/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/client/dist/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/client/dist/index.html"
    }
  ]
}
